{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aacotroneo/laravel-saml2": "^2.1",
        "binshops/laravel-blog": "^9.3",
        "botman/botman": "^2.8",
        "botman/driver-web": "^1.5",
        "creativeorange/gravatar": "~1.0",
        "doctrine/dbal": "^2.0",
        "donatello-za/rake-php-plus": "^1.0",
        "elasticsearch/elasticsearch": "8.10.0",
        "fideloper/proxy": "^4.0",
        "gregwar/captcha": "^1.2",
        "html2text/html2text": "^4.3",
        "kriswallsmith/buzz": "^1.2",
        "laravel/framework": "^9.0",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.5",
        "league/flysystem-aws-s3-v3": "^3.0",
        "league/flysystem-sftp-v3": "^3.28",
        "mailgun/mailgun-php": "^3.6",
        "mishagp/ocrmypdf": "^0.4.0",
        "nlp-tools/nlp-tools": "^0.1.3",
        "nyholm/psr7": "^1.8",
        "owen-it/laravel-auditing": "^12.0",
        "paquettg/php-html-parser": "^3.1",
        "php-extended/php-http-message-factory-psr17": "^6.0",
        "php-http/discovery": "^1.19",
        "popphp/pop-mail": "^3.6",
        "rap2hpoutre/fast-excel": "^5.1",
        "smalot/pdfparser": "^0.14.0",
        "spatie/crawler": "^4.7",
        "spatie/image-optimizer": "^1.2",
        "spatie/pdf-to-text": "^1.4",
        "swiftmailer/swiftmailer": "^6.0",
        "thiagoalessio/tesseract_ocr": "^2.13",
        "unisharp/laravel-filemanager": "^2.9",
        "vedmant/laravel-feed-reader": "^1.6",
        "webpatser/laravel-uuid": "^4.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^6.0",
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
